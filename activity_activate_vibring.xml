<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/ii"
    android:layout_height="match_parent"
    tools:context="com.example.admin.vibring.activatevibring.ActivateVibring">
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/activatevibring"
        android:textStyle="bold"
        android:text="ACTIVATE"
        android:layout_marginBottom="14dp"
        android:layout_above="@+id/deactivatevibring"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/deactivatevibring"
        android:text="DEACTIVATE"
        android:textStyle="bold"
        android:layout_marginBottom="17dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
